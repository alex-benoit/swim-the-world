<div class="container">
  <div class="row">
    <div class="col-xs-12">
      <h1>Challenges</h1>
    </div>
    <% @challenges.each do |challenge| %>
      <div class="col-xs-12 col-sm-6 col-md-4">
        <div class="card">
          <p><%= challenge.name %></p>
        </div>
      </div>
    <% end %>
  </div>
</div>
